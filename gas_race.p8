version 29
__lua__
function _init()
p1 = {x=00, y=103, speed=0.7}
gas = 200
end

function _update()
 mvt()
 fullgas()
end

function _draw()
	cls(4)
	map(0, 0, 0, 0)
	spr (1, p1.x, p1.y)
	spr (9, 17, 1)
		if (gas<150) then 
			spr (25, 17, 1)
		end
	spr (8, 9, 1)
		if (gas<100) then 
			spr (24, 9, 1)
		end
	spr (7, 1, 1)
		if (gas<50) then 
			spr (23, 1, 1)
		end
end 
-->8
-- mouvement joueur

function mvt()
	if (btn(⬆️)) p1.y-=p1.speed
	if (btn(⬇️)) p1.y+=p1.speed
	if (btn(➡️)) p1.x+=p1.speed
	if (btn(⬅️)) p1.x-=p1.speed
	return true
end
-->8
--barre de vie


function fullgas()
	if (btn(⬆️)) gas-=1
	if (btn(⬇️)) gas-=1
	if (btn(➡️)) gas-=1
	if (btn(⬅️)) gas-=1
end


