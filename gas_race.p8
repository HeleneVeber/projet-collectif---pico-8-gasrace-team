temps = 0

function timer()
	if (btn(⬆️)) temps+=3
	if (btn(⬇️)) temps+=3
	if (btn(➡️)) temps+=3
	if (btn(⬅️)) temps+=3
print (timer)
end 

function draw_timer()
  
  camx=mid(0,p1.x-60,256)
	camy=mid(0,p1.y-96,128)
	
  print (temps,camx+ 111,camy+ 12, 7)
end

