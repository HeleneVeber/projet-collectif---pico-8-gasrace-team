--crash

function crash(x, y, w, h)

	if (check_gas(1,x+1, y+1)) then
	 p1.speed = 0 
	 return true
	end 
	if (check_gas(1,x+w-2, y+1)) then
		p1.speed = 0 
	 return true
	end
	if (check_gas(1,x+1, y+h-2)) then
	 p1.speed = 0 
		return true
	end
	if (check_gas(1,x+w-2, y+h-2)) then
		p1.speed = 0 
	 return true
	end
 return false
end



