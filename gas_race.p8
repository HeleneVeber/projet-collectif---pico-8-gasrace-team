--winner

function winner()
	if check_gas(2,p1.x,p1.y) then
		p1.speed = 0 
		afficher_victoire()
	end
end

function afficher_victoire()
	if song == false then
		music(4)
		song = true
		end
	cls(11)
	camera(0,0)
	print("t'es arrive!", 30, 50, 7)
	print("press ❎ to retry", 30, 70, 3)
end




	 
