function _init()

	music (0)
	create_p1()
	create_jauge()
	gas = 350
	temps = 0
end

function _update()
 
	timer()
	
	update_camera()
 
 update_jauge()
 
 mouv_p1()
 
	empty()
 
 retry()
	
end

function _draw()
	
	cls()
	map(0, 0, 0, 0, 128, 64)
	
draw_timer()

draw_p1()
	     
draw_empty()
	
draw_fillgas()
	
game_over_crash()

game_over_meters()
	
game_over_gas()

winner()
	
end 
